{
    "name": "rhs",
    "description": "Interact with rclone with Telegram.",
    "keywords": [
        "telegram",
        "shell",
        "rclone",
        "heroku"
    ],
    "repository": "https://github.com/reaitten/rhs.git",
    "env": {
      "BOT_TOKEN": {
        "description": "The bot token provided by @BotFather",
        "required": true
      },
      "OWNER_ID": {
        "description": "Your Telegram user ID, get it from @userinfobot",
        "value": "",
        "required": true
      },
      "RCLONE_CONFIG_BASE64": {
        "description": "Your Rclone config encoded using Base64",
        "required": false
      }
    },
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ]
  }
